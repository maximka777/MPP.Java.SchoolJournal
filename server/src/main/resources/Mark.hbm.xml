<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="school.journal.entity.Mark" table="mark" schema="school_journal_db">
        <id name="markId" column="mark_id"/>
        <property name="pupilId" column="pupil_id"/>
        <property name="value" column="value"/>
        <property name="type" column="type"/>
        <property name="date" column="date"/>
        <property name="subjectId" column="subject_id"/>
        <property name="teacherId" column="teacher_id"/>
    </class>
</hibernate-mapping>